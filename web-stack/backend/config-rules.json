{
    "exec": {
        "Allow": [
            "^/usr/bin/ls$"
        ]
    },
    "open": {
        "Deny": [
            "^/etc/passwd$",
            "^/etc/shadow/$"
        ]
    },
    "connect": {
        "Allow": [
            "127.0.0.1:[0-9]+"
        ]
    },
    "accept": {
        "Deny": [
            "127.0.0.1:[0-9]+"
        ]
    }
}